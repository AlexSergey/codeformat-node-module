    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
